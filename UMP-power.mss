
#power-line {
  [zoom >= 14] {
    line-width: 0.5;
    line-color: #777;
    a/line-width: 2;
    a/line-color: #777;
    a/line-opacity: 0.5;
    a/line-dasharray: 0,8,5,200;
    a/line-clip:false;
    b/line-width: 3;
    b/line-color: #777;
    b/line-opacity: 0.5;
    b/line-dasharray: 0,10,3,200;
    b/line-clip:false;
    c/line-width: 4;
    c/line-color: #777;
    c/line-opacity: 0.5;
    c/line-dasharray: 0,12,1,200;
    c/line-clip:false;
    d/line-width: 2;
    d/line-color: #777;
    d/line-opacity: 0.5;
    d/line-dasharray: 0,108,5,100;
    d/line-clip:false;
    e/line-width: 3;
    e/line-color: #777;
    e/line-opacity: 0.5;
    e/line-dasharray: 0,108,3,102;
    e/line-clip:false;
    f/line-width: 4;
    f/line-color: #777;
    f/line-opacity: 0.5;
    f/line-dasharray: 0,108,1,104;
    f/line-clip:false;
  }
}

